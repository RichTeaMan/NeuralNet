<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>netstandard1.4</TargetFramework>
    <ApplicationIcon />
    <OutputTypeEx>library</OutputTypeEx>
    <StartupObject />
    <Version>0.1.0-beta</Version>
    <AssemblyVersion>0.1.0.0</AssemblyVersion>
    <FileVersion>0.1.0.0</FileVersion>
    <RepositoryUrl>https://github.com/RichTeaMan/NeuralNet</RepositoryUrl>
    <PackageId>RichTea.NeuralNetLib</PackageId>
    <Authors>Thomas Holmes</Authors>
    <Company>Thomas Holmes</Company>
    <PackageLicenseUrl>https://github.com/RichTeaMan/NeuralNet/blob/master/license.txt</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/RichTeaMan/NeuralNet</PackageProjectUrl>
    <PackageTags>neural net</PackageTags>
    <Copyright>2018 Thomas Holmes</Copyright>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <OutputPath>bin\Debug</OutputPath>
    <DocumentationFile>bin\Debug\netstandard1.4\NeuralNetLib.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <OutputPath>bin\Release</OutputPath>
    <DocumentationFile>bin\Release\netstandard1.4\NeuralNetLib.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="RichTea.Common" Version="0.7.0-beta" />
  </ItemGroup>

</Project>